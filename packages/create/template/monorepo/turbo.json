{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**/*.?{c|m}js"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "stub": {
      "cache": false
    }
  }
}
